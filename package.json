{
	"name": "test-assignment",
	"version": "1.0.0",
	"main": "index.js",
	"author": "Guilherme Vieira <guilherme.paiva@avenuecode.com>",
	"license": "MIT",
	"private": false,
	"scripts": {
		"dev": "webpack './src' --mode development --watch",
		"start": "webpack './src' --mode production && yarn instructions",
		"instructions": "echo \"\\033[33m\n-----------------------\nNow you can run yarn test to run all the available tests!\n-----------------------\n\"",
		"test": "jest --passWithNoTests --runInBand --detectOpenHandles",
		"test:watch": "yarn test --watch",
		"test:unit": "yarn test --watch -c jest-unit.config.ts",
		"test:integration": "yarn test --watch -c jest-integration.config.ts",
		"test:acceptance": "yarn test --watch -c jest-acceptance.config.ts",
		"test:ci": "yarn test --coverage"
	},
	"dependencies": {
		"@babel/core": "^7.15.5",
		"@babel/plugin-transform-modules-commonjs": "^7.15.4",
		"@babel/preset-env": "^7.15.6",
		"babel-jest": "^27.1.1",
		"faker": "^5.5.3",
		"html-webpack-plugin": "^5.3.2",
		"jest": "^27.1.1",
		"puppeteer": "^10.2.0",
		"webpack": "^5.52.0",
		"yup": "^0.32.9"
	},
	"devDependencies": {
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"webpack-cli": "^4.8.0"
	}
}
